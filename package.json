{
  "name": "magnum-opus",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "serve": "docusaurus serve",
    "clear": "docusaurus clear",
    "lint": "eslint src",
    "lint.dryfix": "eslint src --fix-dry-run",
    "lint.fix": "eslint src --fix",
    "lint.fix.staged": "eslint --fix",
    "lint.css": "stylelint --fix",
    "lint.md": "markdownlint --fix --ignore node_modules",
    "lint.staged": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint.fix.staged",
      "prettier"
    ],
    "**/*.{css,scss}": [
      "npm run lint.css",
      "prettier"
    ],
    "**/*.md": [
      "npm run lint.md",
      "prettier"
    ]
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-alpha.70",
    "@docusaurus/preset-classic": "2.0.0-alpha.70",
    "@mdx-js/react": "^1.6.21",
    "clsx": "^1.1.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint.staged",
      "commit-msg": "node scripts/commit-msg.js",
      "pre-push": "npm run test.commiting"
    }
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.0.0-alpha.70",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/react-hooks": "^5.0.3",
    "@testing-library/user-event": "^12.6.3",
    "@tsconfig/docusaurus": "^1.0.2",
    "@types/react": "^17.0.2",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.16.1",
    "docusaurus-plugin-sass": "^0.1.11",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-detailed-reporter": "^0.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-testing-library": "^3.9.2",
    "jest": "^26.6.3",
    "markdownlint-cli": "^0.23.2",
    "prettier": "^2.1.2",
    "stylelint": "^11.1.1",
    "stylelint-declaration-use-variable": "^1.6.0",
    "stylelint-prettier": "^1.1.1",
    "stylelint-scss": "^3.12.1",
    "typescript": "^4.2.2"
  }
}
